{
  "name": "eslint-config-tamia",
  "description": "Tâmia’s ESLint config",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "npm run test:jest",
    "lint": "eslint . --cache --fix",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tamiadev/eslint-config-tamia"
  },
  "files": [
    "rules",
    "index.js",
    "legacy.js",
    "react.js"
  ],
  "keywords": [
    "eslintconfig",
    "tamia"
  ],
  "author": {
    "name": "Artem Sapegin",
    "url": "http://sapegin.me/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tamiadev/eslint-config-tamia/issues"
  },
  "homepage": "https://github.com/tamiadev/eslint-config-tamia",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.6.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "jest": "^21.0.2",
    "prettier": "^1.6.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "semantic-release": "^6.3.6",
    "semantic-release-tamia": "^1.1.1"
  },
  "peerDependencies": {
    "eslint": ">=4",
    "eslint-plugin-prettier": ">=1",
    "prettier": ">=1.3"
  },
  "release": {
    "analyzeCommits": "semantic-release-tamia/analyzeCommits",
    "generateNotes": "semantic-release-tamia/generateNotes",
    "verifyRelease": "semantic-release-tamia/verifyRelease"
  }
}
